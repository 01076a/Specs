{
  "name": "paystandiossdk",
  "version": "0.1.6",
  "summary": "PayStand's iOS SDK.",
  "description": "                       Bringing numerous payment methods to your apps, use PayStand's SDK for iOs to provide a wide range of options to your customers.\n",
  "homepage": "https://www.paystand.com",
  "license": "APACHE",
  "authors": {
    "Joshua Payne": "jpayne@paystand.com",
    "Scott Campbell": "scampbell@paystand.com"
  },
  "source": {
    "git": "https://github.com/paystand/paystandiossdk.git",
    "tag": "0.1.6"
  },
  "platforms": {
    "ios": "6.1"
  },
  "requires_arc": true,
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/Pod/Classes\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/Pod/Classes/paystandiossdk\""
  },
  "source_files": "Pod/Classes/paystandiossdk/*.h",
  "preserve_paths": "Pod/Classes/libpaystandMobileSDK.a",
  "vendored_libraries": "Pod/Classes/libpaystandMobileSDK.a",
  "resource_bundles": {
    "paystandiossdk": [
      "Pod/Assets/*.png"
    ]
  },
  "public_header_files": "Pod/Classes/**/*.h",
  "dependencies": {
    "CardFlight": [
      "~> 1.6"
    ],
    "PPSSignatureView": [
      "~> 0.1"
    ]
  }
}