{
  "name": "YXProgressHUDView",
  "version": "1.0.0",
  "summary": "An iOS activity indicator view.",
  "description": "YXProgressHUDView is an iOS drop-in class that displays a translucent HUD \nwith an indicator and/or labels while work is being done in a background thread. \nThe HUD is meant as a replacement for the undocumented, private UIKit UIProgressHUD \nwith some additional features.",
  "homepage": "https://github.com/caoyunxiao/YXProgressHUDView",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Matej Bukovinski": "chinacgcgcg@163.com"
  },
  "source": {
    "git": "https://github.com/caoyunxiao/YXProgressHUDView.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "source_files": "YXProgressHUDView/*.{h,m}",
  "public_header_files": "YXProgressHUDView/HUD_Configuration.h",
  "frameworks": "UIKit",
  "requires_arc": true,
  "dependencies": {
    "MBProgressHUD": [
      "~> 1.0.0"
    ]
  },
  "resources": "YXProgressHUDView/Resources/*.png"
}
