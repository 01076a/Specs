{
  "name": "AwesomePossum",
  "version": "0.1.2",
  "summary": "A library using the iOS units sensors to determine whether you are you.",
  "description": "This library uses internal sensors to match up your identity by using neural networks to determine likelyhood of you being you",
  "homepage": "https://github.com/brynjen/AwesomePossum",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Brynje Nordli": "brynje@gmail.com"
  },
  "source": {
    "git": "https://github.com/brynjen/AwesomePossum.git",
    "branch": "master",
    "tag": "0.1.2"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "public_header_files": [
    "AwesomePossum/Classes/**/*.{h, hpp}"
  ],
  "source_files": [
    "AwesomePossum/Classes/**/*.{h, m, mm, hpp}"
  ],
  "xcconfig": {
    "ARCHS": "${ARCHS_STANDARD_INCLUDING_64_BIT}"
  },
  "frameworks": [
    "CoreLocation",
    "CoreMotion",
    "CoreImage",
    "AVFoundation",
    "opencv2",
    "AFNetworking",
    "Accelerate",
    "SystemConfiguration"
  ],
  "dependencies": {
    "OpenCV": [
      "~> 3.2"
    ],
    "AFNetworking": [
      "~> 3.0"
    ],
    "Reachability": [
      "~> 3.2"
    ]
  }
}
