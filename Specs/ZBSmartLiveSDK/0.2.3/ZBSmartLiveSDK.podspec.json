    "AFNetworking": [
      "~> 3.0.4"
    ],
    "MJExtension": [
      "~> 3.0.11"
    ]
  },
  "libraries": [
    "z",
    "c++"
  ],
  "subspecs": [
    {
      "name": "Model",
      "source_files": [
        "Pod/Model",
        "Pod/Model/**/*.{h, m}"
      ],
      "public_header_files": [
        "Pod/Model",
        "Pod/Model/**/*.h"
      ]
