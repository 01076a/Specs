{
  "name": "ZBSmartLiveSDK",
  "version": "0.2.3",
  "summary": "ZBSmartLiveSDK for iOS,simple create live app.",
  "homepage": "https://github.com/LipYoung/ZBSmartLiveSDK",
  "license": "MIT",
  "authors": {
    "LipYoung": "mainbundle@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "/Users/lip/Documents/Code/Zhibo_git_repository/zhibo_sdk_source_code",
    "tag": "0.2.3"
  },
  "source_files": [
    "Pod",
    "Pod/*.{h, m}"
