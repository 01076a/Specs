{
  "name": "OpenSSL-iOS-lib",
  "version": "1.0.2l.5",
  "summary": "根据最新官方编译的 OpenSSL iOS 库, 官方网址: https://www.openssl.org/source/",
  "authors": {
    "707689817@qq.com": "707689817@qq.com"
  },
  "homepage": "https://github.com/FredericJacobs/OpenSSL-Pod",
  "source": {
    "git": "https://github.com/LengYi/OpenSSLRepo.git",
    "tag": "1.0.2l.5"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": false,
  "subspecs": [
    {
      "name": "openssl",
      "source_files": "include/openssl/**/*.h",
      "public_header_files": "include/openssl/**/*.h",
      "ios": {
        "preserve_paths": [
          "lib/libcrypto.a",
          "lib/libssl.a"
        ],
        "vendored_libraries": [
          "lib/libcrypto.a",
          "lib/libssl.a"
        ]
      },
      "libraries": [
        "ssl",
        "crypto"
      ],
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/OpenSSL-iOS-lib/include/openssl/**"
      }
    }
  ]
}
