{
  "name": "GoogleAnalytics-Framework",
  "version": "3.15",
  "summary": "GoogleAnalytics-Framework is a dynamic framework wrapper for Google Analytics iOS SDK.",
  "description": "GoogleAnalytics-Framework is a dynamic framework wrapper for Google Analytics iOS SDK. \n  It allows seamless integration of GoogleAnalytics with Swift CocoaPods enforcing use of the \"use_frameworks!\" tag.",
  "homepage": "https://github.com/akashivskyy/GoogleAnalytics-Framework",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Adrian Kashivskyy": ""
  },
  "source": {
    "git": "https://github.com/akashivskyy/GoogleAnalytics-Framework.git",
    "tag": "3.15"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "Google Analytics/**/*.{h, m}",
  "subspecs": [
    {
      "name": "GoogleAnalytics",
      "vendored_libraries": "Google Analytics/External/libGoogleAnalyticsServices.a",
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/Google Analytics/External/**"
      },
      "libraries": [
        "sqlite3",
        "z"
      ],
      "frameworks": [
        "CoreData",
        "CFNetwork",
        "Security",
        "SystemConfiguration"
      ]
    }
  ]
}
